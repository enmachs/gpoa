- names = %w(Enmanuel Luis Gabriel Pedro Anyelo)
#index-container{ :controller => "scripts" }
  .container
    .row
      .listing.col-lg-12.col-xs-12
        %section.content-header.index-header
          %h1{ style: 'display: inline-block' }
            Base de datos
        .box.slice-box
          .box-body.no-padding
            %table.table.table-bordered
              %tbody#objects-container
                %tr
                  %th
                  %th.text-center Alumno
                  %th.text-center Corte I
                  %th.text-center Corte II
                  %th.text-center Corte III
                  %th.text-center Final
                  %th.text-center Aprobado
                  %th.text-center Tipo de variable
                - random_num = []
                - 5.times do |x|
                  %tr
                    %td= "#{x + 1}."
                    %td.text-center= names[x]
                    - 3.times do |num|
                      - a = rand(0.10..0.20).round(2) 
                      - random_num.push(a)
                      %td.text-center= a
                    - final = random_num.reduce(&:+)/3
                    - random_num = []
                    %td.text-center= final
                    - if final >= 0.17
                      %td.text-center Aprobado
                      %td.text-center Continua
                    - else 
                      %td.text-center Reprobado
                      %td.text-center Discreta
                    -# %td= #{final > 0.17 ? 'Aprodabo' : 'Reprobado'}
                    
      .listing.col-lg-12.col-xs-12
        %section.content-header.index-header
          %h1{ style: 'display: inline-block' }
            ProgramaciÃ³n
        .box.slice-box
          .box-body.no-padding
            %table.table.table-bordered
              %tbody#objects-container
                %tr
                  %th
                  %th.text-center Alumno
                  %th.text-center Corte I
                  %th.text-center Corte II
                  %th.text-center Corte III
                  %th.text-center Final
                  %th.text-center Aprobado
                  %th.text-center Tipo de variable
                - 5.times do |x|
                  %tr
                    %td.text-center= "#{x + 1}."
                    %td.text-center= names[x]
                    - 3.times do |num|
                      - a = rand(0.10..0.20).round(2)
                      - random_num.push(a)
                      %td.text-center= a
                    - final = (random_num.reduce(&:+)) / 3
                    - random_num = []
                    %td.text-center= final
                    - if final >= 0.16
                      %td.text-center Aprobado
                      %td.text-center Continua
                    - else 
                      %td.text-center Reprobado
                      %td.text-center Discreta
                    -# %td= #{final > 0.16 ? 'Aprodabo' : 'Reprobado'}


                  

