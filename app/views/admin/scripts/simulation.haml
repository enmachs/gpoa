:javascript
  $(document).ready(function(){
    $('#script').click(function(e) {
      e.preventDefault()
      $('#import_file_script').click()
      $('#import_file_script').change(function() {
        $('.waiting').css('display', 'block')
        $('#import_form_script').submit()
      })
    });
  });
